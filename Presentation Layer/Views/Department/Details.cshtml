@model Department
@{
    ViewData["Title"] = "Details";
}

<h1>Department Details</h1>
<br />
<br />

<div>
    <div class="my-3 form-group">
        <label asp-for="Code"></label>
        <input asp-for="Code" class="form-control" readonly />
    </div>
    <div class="my-3 form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-control" readonly />
    </div>
    <div class="my-3 form-group">
        <label asp-for="CreateAt"></label>
        <input asp-for="CreateAt" class="form-control" readonly />
    </div>
    <div class="my-3">
        <a asp-action="Edit" asp-controller="Department" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
        <a asp-action="Index" asp-controller="Department" class="btn btn-secondary">Back</a>
    </div>
</div>

