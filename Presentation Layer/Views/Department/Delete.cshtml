@model Department
@{
    ViewData["Title"] = "Delete";
}

<h1>Delete Department</h1>

<br />
<br />

<form asp-action="Delete" asp-controller="Department" method="post">
    <div class="my-3 form-group">
        <label asp-for="Code"></label>
        <input asp-for="Code" class="form-control" readonly />
    </div>
    <div class="my-3 form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-control" readonly />
    </div>
    <div class="my-3 form-group">
        <label asp-for="CreateAt"></label>
        <input asp-for="CreateAt" class="form-control" readonly />
    </div>
    <div class="my-3">
        <button type="submit" value="submit" class="btn btn-primary">Delete</button>
        <a asp-action="Index" asp-controller="Department" class="btn btn-secondary">Back</a>
    </div>
</form>



