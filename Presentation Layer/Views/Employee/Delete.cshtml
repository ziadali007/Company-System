@model Employee
@{
    ViewData["Title"] = "Delete";
}

<h1>Delete Employee</h1>

<br />
<br />

<form asp-action="Delete" asp-controller="Employee" method="post">
    <partial name="PartialViews/DetailsDeletePartialView" />
    <input type="hidden" asp-for="ImageName" />
    <div class="my-3">
        <button type="submit" value="submit" class="btn btn-primary">Delete</button>
        <a asp-action="Index" asp-controller="Employee" class="btn btn-secondary">Back</a>
    </div>
</form>